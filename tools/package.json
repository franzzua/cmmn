{
  "name": "@cmmn/tools",
  "version": "1.9.13",
  "description": "Compilation, bundling, code generator, testing.",
  "main": "dist/rollup.config.js",
  "type": "module",
  "repository": {
    "url": "https://github.com/franzzua/cmmn/tree/master/tools"
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {},
  "bin": {
    "cmmn": "bin.js"
  },
  "exports": {
    "./compile/tsconfig.json": "./compile/tsconfig.json",
    "./test": "./test/index.js",
    "./test/config": "./test/jest.config.js"
  },
  "typesVersions": {
    "*": {
      "test": [
        "./test/index.d.ts"
      ],
      ".": [
        "./compile/typings.d.ts"
      ]
    }
  },
  "typings": "compile/typings.d.ts",
  "files": [
    "bin.js",
    "gen/*",
    "compile/*",
    "bundle/*",
    "serve/*",
    "plugins/*",
    "spawn/*",
    "test/*",
    "helpers/*"
  ],
  "dependencies": {
    "@testdeck/jest": "0.2.0",
    "@types/node": "20.x.x",
    "@types/sinon": "10.x.x",
    "@zerollup/ts-helpers": "1.7.18",
    "earl": "^1.1.0",
    "esbuild": "0.19.5",
    "esbuild-plugin-less": "1.3.1",
    "esbuild-register": "3.5.0",
    "esbuild-visualizer": "0.4.1",
    "fast-glob": "^3.2.11",
    "file-uri-to-path": "2.x.x",
    "import-meta-resolve": "2",
    "jasmine-expect": "5.0.0",
    "less": "^4",
    "live-server": "1.2.2",
    "sinon": "17.0.1"
  },
  "author": "",
  "license": "ISC",
  "gitHead": "2755b333808bf97f1d9454601c2a2c9a88e4a021"
}