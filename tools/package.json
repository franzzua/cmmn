{
  "name": "@cmmn/tools",
  "version": "1.5.1",
  "description": "Compilation, bundling, code generator, testing.",
  "main": "dist/rollup.config.js",
  "type": "module",
  "repository": {
    "url": "https://github.com/franzzua/cmmn/tree/master/tools"
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {},
  "bin": {
    "cmmn": "bin.js"
  },
  "exports": {
    "./test": "./test/index.js",
    "./test/config": "./test/jest.config.js"
  },
  "typesVersions": {
    "*": {
      "test": [
        "./test/index.d.ts"
      ],
      ".": [
        "./compile/typings.d.ts"
      ]
    }
  },
  "typings": "compile/typings.d.ts",
  "files": [
    "bin.js",
    "gen/*",
    "compile/*",
    "bundle/*",
    "plugins/*",
    "test/*"
  ],
  "dependencies": {
    "@jest/globals": "27.x.x",
    "@open-wc/rollup-plugin-html": "^1.2.5",
    "@rollup/plugin-alias": "3",
    "@rollup/plugin-commonjs": "^21",
    "@rollup/plugin-json": "4",
    "@rollup/plugin-node-resolve": "^13",
    "@rollup/plugin-replace": "4.x.x",
    "@rollup/plugin-typescript": "^8",
    "@swc/jest": "0.2.17",
    "@testdeck/jest": "0.2.0",
    "@types/jest": "27.x.x",
    "@types/sinon": "10.x.x",
    "@web/rollup-plugin-html": "^1.10.1",
    "fast-glob": "^3.2.11",
    "jest": "27.x.x",
    "less": "^4",
    "rollup": "^2",
    "rollup-plugin-img": "1.x.x",
    "rollup-plugin-livereload": "^2.0.5",
    "rollup-plugin-node-builtins": "^2.1.2",
    "rollup-plugin-node-globals": "^1.4.0",
    "rollup-plugin-postcss": "4.x.x",
    "rollup-plugin-serve": "^1.1.0",
    "rollup-plugin-string": "^3.0.0",
    "rollup-plugin-styles": "^4",
    "rollup-plugin-terser": "^7",
    "rollup-plugin-visualizer": "^5.5.4",
    "sinon": "10.x.x",
    "ts-jest": "27.x.x",
    "ttypescript": "1.5.13",
    "typescript": "4.x.x",
    "typescript-transform-paths": "^3.3.1"
  },
  "author": "",
  "license": "ISC",
  "gitHead": "2dc32e552dd5877be0ceb18f64cb896f13f15303"
}
