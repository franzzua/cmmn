{
  "name": "@cmmn/tools",
  "version": "1.8.5",
  "description": "Compilation, bundling, code generator, testing.",
  "main": "dist/rollup.config.js",
  "type": "module",
  "repository": {
    "url": "https://github.com/franzzua/cmmn/tree/master/tools"
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {},
  "bin": {
    "cmmn": "bin.js"
  },
  "exports": {
    "./compile/tsconfig.json": "./compile/tsconfig.json",
    "./test": "./test/index.js",
    "./test/config": "./test/jest.config.js"
  },
  "typesVersions": {
    "*": {
      "test": [
        "./test/index.d.ts"
      ],
      ".": [
        "./compile/typings.d.ts"
      ]
    }
  },
  "typings": "compile/typings.d.ts",
  "files": [
    "bin.js",
    "gen/*",
    "compile/*",
    "bundle/*",
    "serve/*",
    "plugins/*",
    "spawn/*",
    "test/*"
  ],
  "dependencies": {
    "@jest/globals": "27.x.x",
    "@modular-css/rollup": "28",
    "@open-wc/rollup-plugin-html": "^1.2.5",
    "@rollup/plugin-alias": "3",
    "@rollup/plugin-commonjs": "^21",
    "@rollup/plugin-image": "2",
    "@rollup/plugin-json": "4",
    "@rollup/plugin-node-resolve": "^13",
    "@rollup/plugin-replace": "4.x.x",
    "@rollup/plugin-typescript": "^8",
    "@swc/jest": "0.2.17",
    "@testdeck/jest": "0.2.0",
    "@types/jest": "27.x.x",
    "@types/sinon": "10.x.x",
    "@web/rollup-plugin-html": "^1.10.1",
    "fast-glob": "^3.2.11",
    "file-uri-to-path": "2.x.x",
    "import-meta-resolve": "2",
    "jest": "29.x.x",
    "less": "^4",
    "live-server": "1.2.2",
    "postcss-assets": "6",
    "postcss-import": "14",
    "postcss-modules": "*",
    "rollup": "^2",
    "rollup-plugin-copy": "3",
    "rollup-plugin-livereload": "^2.0.5",
    "rollup-plugin-node-builtins": "^2.1.2",
    "rollup-plugin-node-globals": "^1.4.0",
    "rollup-plugin-serve": "^1.1.0",
    "rollup-plugin-sourcemaps": "^0.6.3",
    "rollup-plugin-string": "^3.0.0",
    "rollup-plugin-styles": "^4",
    "rollup-plugin-terser": "^7",
    "rollup-plugin-visualizer": "^5.5.4",
    "sinon": "10.x.x",
    "ts-jest": "29.x.x",
    "typescript-transform-paths": "3.4.6",
    "unique-slug": "*"
  },
