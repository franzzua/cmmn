{
  "name": "@cmmn/cell",
  "version": "1.8.3",
  "description": "pull-based state flow",
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "dist"
  ],
  "type": "module",
  "main": "./dist/bundle/index.js",
  "browser": "./dist/bundle/lib.js",
  "exports": {
    ".": {
      "types": "./dist/typings/index.d.ts",
      "import": "./dist/bundle/index.js",
      "require": "./dist/bundle/index.cjs"
    },
    "./lib": {
      "types": "./dist/typings/min.d.ts",
      "import": "./dist/bundle/lib.js",
      "require": "./dist/bundle/lib.js"
    }
  },
  "scripts": {
    "build": "cmmn compile --watch",
    "bundle": "cmmn bundle --watch",
    "bundle:lib": "cmmn bundle lib --watch",
    "bundle:lib:prod": "cmmn bundle lib --prod"
  },
  "dependencies": {
    "@cmmn/core": "^1.8.3"
  },
  "devDependencies": {
    "@cmmn/tools": "^1.8.1"
  },
  "author": "",
  "license": "ISC",
  "gitHead": "2755b333808bf97f1d9454601c2a2c9a88e4a021",
  "repository": {
    "url": "https://github.com/franzzua/cmmn/tree/master/cell"
  },
  "cmmn": {
    "index": {
      "input": "dist/esm/index.js",
      "module": "es,cjs",
      "external": [
        "@cmmn.*"
      ]
    },
    "lib": {
      "input": "dist/esm/lib.js",
      "module": "es",
      "alias": {
        "@cmmn/core": "@cmmn/core/min"
      }
    }
  }
}
