{
  "name": "@cmmn/sync",
  "version": "1.3.0",
  "description": "collaborative with yjs",
  "exports": {
    "./proxy": {
      "import": "./dist/proxy.js",
      "require": "./dist/proxy.cjs",
      "default": "./dist/proxy.js"
    },
    "./worker": {
      "import": "./dist/worker.js",
      "require": "./dist/worker.cjs",
      "default": "./dist/worker.js"
    },
    "./package.json": "./package.json"
  },
  "typesVersions": {
    "*": {
      "proxy": [
        "./dist/typings/entry/proxy.d.ts"
      ],
      "worker": [
        "./dist/typings/worker/index.d.ts"
      ]
    }
  },
  "scripts": {
    "bundle": "cmmn bundle -b --watch",
    "build": "cmmn compile -b --watch"
  },
  "files": [
    "dist"
  ],
  "dependencies": {
    "@cmmn/core": "^1.3.0",
    "simple-peer": "9.x.x",
    "y-protocols": "1.x.x",
    "yjs": "13.x.x"
  },
  "devDependencies": {
    "@cmmn/tools": "^1.3.0"
  },
  "author": "",
  "license": "ISC",
  "cmmn": {
    "client": {
      "input": "./dist/esm/client/index.js"
    },
    "server": {
      "input": "./dist/esm/server/index.js"
    }
  }
}
