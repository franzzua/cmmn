{
  "name": "@cmmn/sync",
  "version": "1.4.0",
  "description": "collaborative with yjs",
  "publishConfig": {
    "access": "public"
  },
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs",
      "default": "./dist/index.js"
    },
    "./webrtc/client": {
      "import": "./dist/webrtc.js",
      "require": "./dist/webrtc.cjs",
      "default": "./dist/webrtc.js"
    },
    "./webrtc/server": {
      "import": "./dist/server.js",
      "require": "./dist/server.cjs",
      "default": "./dist/server.js"
    },
    "./package.json": "./package.json"
  },
  "typesVersions": {
    "*": {
      "proxy": [
        "./dist/typings/entry/proxy.d.ts"
      ],
      "worker": [
        "./dist/typings/worker/index.d.ts"
      ]
    }
  },
  "scripts": {
    "bundle": "cmmn bundle -b --watch",
    "build": "cmmn compile -b --watch"
  },
  "files": [
    "dist"
  ],
  "dependencies": {
    "@cmmn/core": "^1.4.0",
    "simple-peer": "9.x.x",
    "y-indexeddb": "9.x.x",
    "y-protocols": "1.x.x",
    "yjs": "13.x.x"
  },
  "devDependencies": {
    "@cmmn/tools": "^1.4.0"
  },
  "author": "",
  "license": "ISC",
  "type": "module",
  "cmmn": {
    "index": {
      "input": "./dist/esm/index.js",
      "external": "cellx",
      "module": "es,cjs"
    },
    "webrtc": {
      "input": "./dist/esm/webrtc/client/index.js"
    },
    "server": {
      "input": "./dist/esm/webrtc/server/index.js",
      "module": "es,cjs",
      "external": [
        "node_modules",
        "@cmmn"
      ]
    }
  },
  "gitHead": "824d290afabaa3a44ccb98322cb1758825bd9efb"
}
