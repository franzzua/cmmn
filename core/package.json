{
  "name": "@cmmn/core",
  "version": "1.4.0",
  "description": "di, base extensions, useful functions",
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "dist"
  ],
  "type": "module",
  "typings": "dist/typings/index.ts",
  "typesVersions": {
    "*": {
      "/": [
        "dist/typings/index.ts"
      ]
    }
  },
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "scripts": {
    "build": "cmmn compile --watch",
    "bundle": "cmmn bundle --watch"
  },
  "devDependencies": {
    "@cmmn/tools": "^1.4.0"
  },
  "dependencies": {
    "@js-temporal/polyfill": "^0.3.0",
    "bind-decorator": "^1.0.11",
    "cellx": "^1.10.26",
    "flexid": "0.8.3",
    "msgpackr": "^1.5.2",
    "ulid": "2.x.x"
  },
  "author": "",
  "license": "ISC",
  "gitHead": "76478b70f43752f9bfe6cb96b4e540b1ff1e52e3",
  "cmmn": {
    "index": {
      "input": "dist/esm/index.js",
      "module": "es,cjs",
      "external": [
        "temporal/polyfill",
        "cellx"
      ]
    }
  }
}
