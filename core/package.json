      "default": "./dist/bundle/index.js"
    }
  },
  "scripts": {
    "build": "cmmn compile --watch",
    "bundle": "cmmn bundle --watch"
  },
  "devDependencies": {
    "@cmmn/tools": "workspace:*"
  },
  "dependencies": {
    "bind-decorator": "^1.0.11"
  },
  "author": "",
  "license": "ISC",
  "gitHead": "2755b333808bf97f1d9454601c2a2c9a88e4a021",
  "cmmn": {
    "index": {
      "input": "dist/esm/index.js",
      "module": "es,cjs",
      "platform": "neutral",
      "external": [
        "@cmmn"
      ]
    }
  }
