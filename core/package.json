{
  "name": "@cmmn/core",
  "version": "1.4.12",
  "description": "di, base extensions, useful functions",
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "dist"
  ],
  "type": "module",
  "main": "./dist/index.js",
  "typings": "./dist/typings/index.d.ts",
  "typesVersions": {
    "*": {
      "/": [
        "dist/typings/index.ts"
      ]
    }
  },
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "scripts": {
    "build": "cmmn compile --watch",
    "bundle": "cmmn bundle --watch"
  },
  "devDependencies": {
    "@cmmn/tools": "^1.4.12"
  },
  "dependencies": {
    "@types/luxon": "2.x.x",
    "bind-decorator": "^1.0.11",
    "luxon": "2.x.x",
    "msgpackr": "^1.5.2"
  },
  "author": "",
  "license": "ISC",
  "gitHead": "6d71154c76d97bf9e1a8ebd52ab3c31df797d56d",
  "cmmn": {
    "index": {
      "input": "dist/esm/index.js",
      "module": "es,cjs",
      "external": [
        "temporal/polyfill",
        "cellx"
      ]
    }
  }
}
