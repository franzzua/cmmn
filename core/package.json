{
  "name": "@cmmn/core",
  "version": "1.9.12",
  "repository": {
    "url": "https://github.com/franzzua/cmmn/tree/master/core"
  },
  "description": "di, base extensions, useful functions",
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "dist"
  ],
  "type": "module",
  "main": "./dist/bundle/index.js",
  "module": "./dist/esm/index.js",
  "exports": {
    ".": {
      "types": "./dist/typings/index.d.ts",
      "import": "./dist/bundle/index.js",
      "require": "./dist/bundle/index.cjs",
      "default": "./dist/bundle/index.js"
    },
    "./min": {
      "types": "./dist/typings/min.d.ts",
      "import": "./dist/bundle/min.js",
      "require": "./dist/bundle/min.cjs",
      "default": "./dist/bundle/min.js"
    }
  },
  "typesVersions": {
    "*": {
      "min": [
        "dist/typings/min.d.ts",
        "extensions.d.ts"
      ],
      "*": [
        "dist/typings/index.d.ts",
        "extensions.d.ts"
      ]
    }
  },
  "scripts": {
    "build": "cmmn compile --watch",
    "bundle": "cmmn bundle --watch"
  },
  "devDependencies": {
    "@cmmn/tools": "1.9.12"
  },
  "dependencies": {
    "@types/luxon": "2.x.x",
    "bind-decorator": "^1.0.11",
    "luxon": "2.x.x",
    "msgpackr": "^1.5.2"
  },
  "author": "",
  "license": "ISC",
  "gitHead": "2755b333808bf97f1d9454601c2a2c9a88e4a021",
  "cmmn": {
    "index": {
      "input": "dist/esm/index.js",
      "module": "es,cjs",
      "platform": "neutral",
      "external": [
        "luxon",
        "msgpackr/pack",
        "@cmmn"
      ]
    },
    "min": {
      "input": "dist/esm/min.js",
      "platform": "neutral",
      "module": "es,cjs"
    }
  }
}