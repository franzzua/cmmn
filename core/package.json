{
  "name": "@cmmn/core",
  "version": "2.0.0",
  "repository": {
    "url": "https://github.com/franzzua/cmmn/tree/master/core"
  },
  "description": "di, base extensions, useful functions",
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "dist"
  ],
  "type": "module",
  "main": "./dist/bundle/index.js",
