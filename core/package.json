{
  "name": "@cmmn/core",
  "version": "1.4.19",
  "description": "di, base extensions, useful functions",
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "dist"
  ],
  "type": "module",
  "main": "./dist/index.js",
  "typings": "index.d.ts",
  "typesVersions": {
    "*": {
      "index.d.ts": [
        "dist/typings/index.d.ts"
      ]
    }
  },
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "scripts": {
    "build": "cmmn compile --watch",
    "bundle": "cmmn bundle --watch"
  },
  "devDependencies": {
    "@cmmn/tools": "^1.4.17",
    "crc-32": "1.x.x"
  },
  "dependencies": {
    "@cmmn/cell": "^1.4.19",
    "@types/luxon": "2.x.x",
    "bind-decorator": "^1.0.11",
    "luxon": "2.x.x",
    "msgpackr": "^1.5.2"
  },
  "author": "",
  "license": "ISC",
  "gitHead": "2dc32e552dd5877be0ceb18f64cb896f13f15303",
  "cmmn": {
    "index": {
      "input": "dist/esm/index.js",
      "module": "es,cjs",
      "external": [
        "luxon"
      ]
    }
  }
}
